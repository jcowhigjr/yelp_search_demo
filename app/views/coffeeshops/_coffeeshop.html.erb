
    <div class="col xl l6 m6 s12">
        <div class="card large coffeeshop-card">
            <div class="card-image">
                 <%= image_tag coffeeshop.image_url, class: "card-img responsive-img" %>
            </div>
            <div class="card-content">
                <span class="card-title truncate"><%= coffeeshop.name %></span>
                <div class="row center">
                    <p class= "truncate"> <i class="material-icons">place</i>   <%= coffeeshop.address %></p>
                </div>
                <div class="card-action row center">
                    <%= link_to "More Info", coffeeshop_path(coffeeshop), class: "btn-large"%>
                </div>
            </div>
        </div>
    </div>